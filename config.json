{
  "app": {
    "name": "app",
    "port": "8080"
  },
  "db": {
    "host": "127.0.0.1",
    "port": "3306",
    "user": "root",
    "pass": "root",
    "schema": "event_management",
    "maxIdleConn": 1,
    "maxOpenConn": 2,
    "maxConnLifetime": 30,
    "debug": true
  },
  "redis": {
    "host": "127.0.0.1",
    "port": "6379",
    "pass": "",
    "db": 0,
    "mandatoryPrefix": "event_management_",
    "accessUuidPrefix": "access-uuid_",
    "refreshUuidPrefix": "refresh-uuid_",
    "userPrefix": "user_",
    "permissionPrefix": "permissions_",
    "userCacheTTL": 3600,
    "permissionCacheTTL": 86400
  },
  "asynq": {
    "redisAddr": "127.0.0.1:6379",
    "db": 15,
    "pass": "*****",
    "concurrency": 10,
    "queue": "event-management",
    "retention": 168,
    "retryCount": 25,
    "delay": 0
  },
  "logger": {
    "filePath": "app.log"
  },
  "jwt": {
    "accessTokenSecret":  "access_token",
    "refreshTokenSecret":  "refresh_token",
    "accessTokenExpiry": 3600,
    "refreshTokenExpiry": 3600
  }
}